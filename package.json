  },
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git://github.com/pascencio/gulp-awspublish.git"
  },
  "scripts": {
    "lint": "prettier --single-quote --write **/*.js && eslint --fix .",
    "pretest": "npm run lint",
