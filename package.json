    "plugin-error": "^1.0.1",
    "through2": "^4.0.2",
    "vinyl": "^2.2.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "concurrent-transform": "^1.0.0",
    "coveralls": "^3.1.0",
    "eslint": "^7.2.0",
    "event-stream": "^4.0.1",
    "gulp": "^4.0.0",
    "gulp-rename": "^2.0.0",
    "istanbul": "^0.4.5",
    "mocha": "^8.4.0",
