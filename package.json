  "keywords": [
    "gulpplugin",
    "aws",
    "s3",
    "publish"
  ],
  "homepage": "https://github.com/pascencio/gulp-awspublish",
  "bugs": "https://github.com/pascencio/gulp-awspublish/issues",
  "author": {
